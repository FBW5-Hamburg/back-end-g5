    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%- include('partials/htmlCss')%>
</head>

<body>
    <%- include('partials/htmlHeader', {error:true, active:'home'})%>

            <div class="site-section-cover overlay" style="background-image: url('../images/hero_bg.jpg');">

                <div class="container">
                    <div class="row align-items-center justify-content-center">
                        <div class="col-lg-10 text-center">
                            <h1>The <strong>ADMIN</strong> Of <strong>Tutorials</strong></h1>
                        </div>
                    </div>
                </div>
            </div>

            <div class="site-section bg-light">
                <div class="container">
                    <div class="row justify-content-center">
                        <div class="col-lg-7 text-center mb-5">
                            <div class="heading">
                                <span class="caption">Edite Password</span>
                                <h2>Change Admin Password</h2>
                            </div>
                        </div>
                    </div>
                    <div class="row signup">
                        <form action="/adminPanel/editAdmin" method="post" >
                            <label for="emial">Write Hier The Old Email:</label><br>
                            <input type="email" name="oldEmail"  placeholder='Old Email'><br> <br>
                            <label for="password">Write Hier The Old Password:</label><br>
                            <input type="password" name="oldPassword" id="" placeholder='Old Password'><br> <br>
                            <input type="email" name="email" id="" placeholder='New Email' ><br> <br>
                            <input type="password" name="password" id="" placeholder='New Password' ><br> <br>
                            <input type="submit" class="btn btn-dark" style="width: 180px;" value="Save">
                        </form>
                    </div>
                </div>
            </div>

            <%- include('partials/htmlFooter')%>


    </div>

    <%- include('partials/htmlJs')%>


